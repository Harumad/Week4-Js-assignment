class profileTemplate{
  constructor(name,email,address,password,age){
    this.name= name
    this.email =email
    this.address= address
    this.password= password
    this.age= age
  }
}

const user1 = new profileTemplate('Ema','emau@gmail.com','Tema com 22','agsuwcxc',20)
console.log(user1);

const user2 = new profileTemplate('mark','maremau@gmail.com','Tema com 2','sftwcxc',40)
console.log(user2);


class Car{
  constructor(wheels,stear,seat,engine){
    this.wheels =wheels
    this.stear =stear
    this.seat= seat
    this.engine= engine
  }
}

// INHERITANCE

class PickUp extends Car{
  features() {
    return this.wheels + "" + this.engine + " "+ this.seat
  }

}

const hilux = new PickUp(4,1,5,1);
console.log(hilux.features());



